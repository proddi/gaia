<div class="pageheader">
{{ if authorized }}
    <div style="float: right">
        {{ user->name }} (<a href="{{ baseUrl . 'logout' }}">logout</a>)
    </div>
    <h2>auth example for user: {{ user->name }}</h2>
{{ else }}
    <h2>auth example (not logged in)</h2>
{{ end }}
</div>

{ { content } }
